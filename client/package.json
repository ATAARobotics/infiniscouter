{
  "name": "infiniscouter",
  "module": "src/app.tsx",
  "type": "module",
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@mui/joy": "^5.0.0-beta.11",
    "preact": "^10.17.1",
    "preact-iso": "^2.3.1",
    "preact-render-to-string": "^6.2.1",
    "prettier": "^3.0.3"
  },
  "devDependencies": {
    "@types/react": "^18.2.33",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "esbuild": "^0.19.2",
    "eslint": "^8.49.0",
    "eslint-plugin-jsdoc": "^46.6.0",
    "eslint-plugin-react": "^7.33.2",
    "typescript": "^5.2.2"
  },
  "scripts": {
    "dev": "esbuild --bundle --watch --sourcemap --outdir=dist --format=esm src/app.tsx --alias:react=preact/compat --alias:react-dom=preact/compat",
    "build": "esbuild --bundle --sourcemap --splitting --minify --target=es2017 --outdir=dist --format=esm source/app.tsx",
    "lint": "tsc --noEmit --pretty && eslint src/*.tsx",
    "format": "prettier -w src/**"
  }
}
